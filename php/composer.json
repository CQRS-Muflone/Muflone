{
    "name": "muflone/muflone-php",
    "description": "PHP port of Muflone - A CQRS and Event Sourcing library",
    "type": "library",
    "keywords": ["cqrs", "event-sourcing", "ddd", "domain-driven-design", "aggregate", "event-store"],
    "license": "MIT",
    "authors": [
        {
            "name": "Muflone Contributors"
        }
    ],
    "require": {
        "php": ">=8.2",
        "psr/log": "^3.0",
        "ramsey/uuid": "^4.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Muflone\\": "src/Muflone/",
            "BrewUp\\": "examples/BrewUp/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muflone\\Tests\\": "tests/"
        },
        "files": [
            "tests/TestHelpers.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
