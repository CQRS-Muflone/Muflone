<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Description>A CQRS and event store library</Description>
    <Copyright>Alessandro Colla and contributors</Copyright>
    <PackageProjectUrl>https://github.com/CQRS-Muflone/Muflone/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/CQRS-Muflone/Muflone/</RepositoryUrl>
    <PackageTags>CQRS, Event sourcing, Event store</PackageTags>
    <AssemblyVersion>0.8.1.1</AssemblyVersion>
    <FileVersion>0.8.1.1</FileVersion>
    <PackageLicenseUrl>https://github.com/CQRS-Muflone/Muflone/blob/master/LICENSE</PackageLicenseUrl>
    <Version>0.9</Version>
    <Authors>Alessandro Colla</Authors>
    <Company>CQRS-Muflone</Company>
    <NeutralLanguage>en</NeutralLanguage>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

</Project>
